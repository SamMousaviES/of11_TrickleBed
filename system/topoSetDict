FoamFile
{
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

actions
(
    {
        name    inletWater;
        type    faceSet;
        action  new;
        source  boxToFace;
        box     (0 0.9999 -0.2) (0.05 1.0001 0.2);
    }

    {
        name    inletWater;
        type    faceZoneSet;
        action  new;
        source  setToFaceZone;
        set     inletWater;
    }
    
    {
        name    inletAir;
        type    faceSet;
        action  new;
        source  boxToFace;
        box     (0.05 0.9999 -0.2) (0.15 1.0001 0.2);
    }

    {
        name    inletAir;
        type    faceZoneSet;
        action  new;
        source  setToFaceZone;
        set     inletAir;
    }

);

